local Notification = require(game:GetService("ReplicatedStorage").Notification)
Notification.new("<Color=Cyan>Xin chào bạn<Color=/>"):Display()
wait(0.5)
Notification.new("<Color=Yellow>Thật tiếc , chức năng này đang update rồi<Color=/>"):Display()
wait(1)
Notification.new("<Color=Cyan>Tham gia group để cập nhật<Color=/>"):Display()
wait(1)
Notification.new("<Color=Yellow>Chúc các bạn sử dụng vui vẻ<Color=/>"):Display()
wait(1)
repeat
    wait()
until game.Players.LocalPlayer
