repeat wait() until game:IsLoaded() and game.Players.LocalPlayer

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local key = getgenv().Key or nil
local hwid = gethwid and gethwid() or tostring(Players.LocalPlayer.UserId)

if not key then
    Players.LocalPlayer:Kick("ChÆ°a nháº­p key duma báº¡n cÃ²n khÃ´ng cÃ³ thÃ¬ mua key ib 0765520260.")
    return
end

local url = "https://dichvuxyz.site/api/checkhwid.php?key=" .. key .. "&hwid=" .. hwid

local success, response = pcall(function()
    return HttpService:JSONDecode(game:HttpGet(url))
end)

if not success then
    Players.LocalPlayer:Kick("Error server , pls ib admin.")
    return
end

if response.status ~= "true" then
    Players.LocalPlayer:Kick("Dit me may chet me may di"))
    return
end

