local b1 = UI["b"]
local submit = UI["9"]
b1.MouseButton1Down:Connect(function()
    setclipboard("https://yeumoney.com/WMytfqY")
end)
submit.MouseButton1Down:Connect(function()
    local gg = UI["7"].Text
    if checkKey(gg) then
        UI["7"].Text = "Key Đúng!"
        wait(3)
        UI["1"]:Destroy()
    else
        UI["7"].Text = "Key Sai!! vui lòng nhập lại"
        spawn(function()
            wait(1)
            UI["7"].Text = ""
        end)
    end
end)
