-- Add a config at the top
local config = {
    autoKey = "Key_scripttonghop213456789", -- Set your key here
    autoStart = true -- Set this to true to start without UI
}

-- Check if the key is correct
local function checkKey(key)
    if key == "Key_scripttonghop213456789" then
        print("[+]: Key đúng!!!")
        -- Run the additional code if the key is correct
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ccditmethkskider/Ducv4/refs/heads/main/hoppremium"))()
        return true
    else
        print("[+]: Key Sai!!!")
        return false
    end
end

-- If autoStart is enabled, check the key from config
if config.autoStart then
    if checkKey(config.autoKey) then
        return -- Exit the script if the key is correct, skip showing the UI
    end
end

-- If key is incorrect or autoStart is disabled, show the UI (No change to the UI)
local viewportSize = game:GetService("Workspace").Camera.ViewportSize
local UI = {}

